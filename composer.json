{
    "name": "scoobydam/file-archiver",
    "description": "Simple PHP file(s) compressor and archiver",
    "type": "library",
    "keywords": ["archiver", "archive", "archives", "zip", "unzip", "compression", "compressor", "compress", "uncompress", "tar", "gz", "tar gz", "bz", "files", "file", "easy", "simple", "light"],
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Damien",
            "email": "damien.vauchel@pm.me"
        }
    ],
    "require": {
        "php": ">=7.3.0"
    },
    "require-dev": {
        "phpro/grumphp": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpstan/phpstan": "^0.12.83",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "platform": {
            "php": "7.3"
        }
    },
    "autoload": {
        "psr-4": {
            "ScoobyFileArchiver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScoobyFileArchiver\\Tests\\": "tests/"
        }
    }
}
